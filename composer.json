{
	"name": "stefna/open-api-bundler",
	"description": "Cli tool to help inline/bundle open api specifications",
	"license": "MIT",
	"type": "library",
	"authors": [
		{
			"name": "Andreas Sundqvist",
			"email": "andreas@stefna.is"
		}
	],
	"require": {
		"php": "^8.2",
		"stefna/json-pointer": "^2.7",
		"psr/log": "^1.1 || ^2.0 || ^3.0",
		"circli/console": "^2.0.0"
	},
	"require-dev": {
		"stefna/codestyle": "^1.17",
		"phpstan/phpstan": "^2.0",
		"starburst/contract": "^1.0",
		"stefna/di": "^1.0",
		"phpunit/phpunit": "^11.4"
	},
	"autoload": {
		"psr-4": {
			"Stefna\\OpenApiBundler\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Stefna\\OpenApiBundler\\Tests\\": "tests/"
		}
	},
	"bin": [
		"bin/bundle",
		"bin/inline"
	]
}
